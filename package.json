{
  "name": "learn-japanese",
  "version": "1.0.0",
  "main": "server.js",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "testBackend": "ava api/tests/ --verbose",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "build": "npm run build:client && npm run build:server & npm run build:serverjsFile",
    "build:client": "next build",
    "build:server": "babel ./api --out-dir .next/api",
    "build:serverjsFile": "babel server.js --out-dir .next/",
    "start-prod": "NODE_ENV=production node initProd.js",
    "start:client": "nodemon server.js --exec babel-node",
    "start:server": "nodemon ./api/index.js --exec babel-node",
    "start": "NODE_ENV=production node server.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@types/rebass": "^0.2.22",
    "@zeit/next-typescript": "^1.0.1",
    "apollo-boost": "^0.1.23",
    "apollo-server": "^2.4.6",
    "apollo-server-express": "^2.4.6",
    "auth0-js": "^9.10.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dayjs": "^1.8.6",
    "express": "^4.16.4",
    "filepath": "^1.1.0",
    "final-form": "^4.11.0",
    "final-form-arrays": "^1.1.1",
    "graphql": "^14.1.1",
    "graphql-server-express": "^1.4.0",
    "graphql-tools": "^4.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.4",
    "next": "^7.0.2",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "prettier": "^1.15.3",
    "react": "^16.2.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.2.0",
    "react-final-form": "^4.0.2",
    "react-final-form-arrays": "^2.0.1",
    "react-redux": "^5.0.7",
    "rebass": "2.0.0-9",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "rodal": "^1.6.3",
    "sequelize": "^4.42.0",
    "styled-components": "3.3.3",
    "styled-react-modal": "^1.1.2",
    "styled-system": "^3.1.6",
    "system-components": "^3.0.1",
    "ts-node": "^8.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/next": "^2.4.7",
    "@types/react": "^16.0.36",
    "ava": "^1.3.1",
    "babel-jest": "^24.3.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "husky": "^1.3.1",
    "jest": "^24.3.0",
    "nodemon": "^1.18.8",
    "nyc": "^13.3.0",
    "pretty-quick": "^1.8.0",
    "proxyquire": "^2.1.0",
    "react-test-renderer": "^16.8.4",
